{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".vinxi/**", ".output/**", "dist/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"prebuild": {
			"cache": false,
			"outputs": ["android/**", "ios/**"]
		},
		"check": {
			"cache": false
		},
		"lint": {
			"dependsOn": ["^build"],
			"cache": false
		},
		"format": {
			"cache": false
		},
		"type-check": {
			"dependsOn": ["^build"]
		},
		"clean": {
			"cache": false
		}
	}
}
